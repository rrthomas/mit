from smite_core.vm_data import Registers

print('// Auto-generated by gen-registers - DO NOT EDIT.\n')
for key in sorted(Registers.__members__.keys()):
    register = Registers[key].value
    print("R", end='')
    if register.read_only:
        print("_RO", end='')
    print("({}, {}, {})".format(key, register.ty, register.uty))
